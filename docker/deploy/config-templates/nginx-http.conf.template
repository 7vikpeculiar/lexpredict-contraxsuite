server {

    server_name ${DOCKER_NGINX_SERVER_NAME};

    listen 8080;

    charset     utf-8;
    client_max_body_size 1024M;
    sendfile on;
    keepalive_timeout 0;

    # this is the internal Docker DNS
    resolver 127.0.0.11;

${NGINX_EXTERNAL_ROUTES}

    include /etc/nginx/server_include/*.conf;

}
